{
  "editor.tabSize": 2,
  "prettier.requireConfig": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "path-autocomplete.extensionOnImport": true,
  "stylelint.validate": ["css", "scss", "vue"],
  "alias-skip.mappings": {
    "@": "/src"
  },
  "path-autocomplete.pathMappings": {
    "@": "${folder}/src"
  }
}